<nav class="#ffff00 yellow accent-2">
    <div class="nav-wrapper">
      <a href="/" class="brand-logo center black-text"><i class="small material-icons">lightbulb_outline</i>Break It Down</a>     
      <ul class="left hide-on-med-and-down">
        <li class="active"><a class="black-text" href="/">Home</a></li>
        <li><a class="black-text" href="/submit">Submit</a></li>
        <li><a class="black-text" href="/login">Login</a></li> 
      </ul> 
   </div>
  </nav>
<div id="home">
  <div class="container">
    <div class="row">
      <div class="col-10 offset-1">
        <h3 class="center-align">{{msg}}</h3>
        <h5>Project Ideas</h5>
        <ul id="example-list" class="list-group">
          {{#each examples}}{{#unless this.members_only}}
          <li style="line-height:1.5" data-id="{{this.id}}" class="list-group-item">
            <div>
              <a href="ideas/{{this.id}}">{{this.title}}</a>
              <br>
              <span style="font-size:12px"> {{this.description}}<br> <b>Technologies used:</b> {{this.tech_used}}
                <div>
                  <b>User Stories:</b>
                  {{#each userstories}}
                  <ul>
                    <li>{{story}}</li>
                  </ul>
                  {{/each}}
                </div>
              </span>
            </div>

          </li>
          {{/unless}}{{/each}}
        </ul>
      </div>
    </div>
  </div>
</div>

<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="https://www.paypalobjects.com/api/checkout.js"></script>
